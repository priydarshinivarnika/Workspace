<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ust.model.Skill" table="SKILLDATA">
		<meta attribute="class-description">This class contains the employee's skill detail.</meta>
		<id name="id" type="int" column="Skill_Id">
			<generator class="identity"></generator>
		</id>
		<property name="name" column="Skill_Name" type="string" />
		<many-to-one name="empDatas" class="com.ust.model.Employee" fetch="select">
            <column name="EMP_ID" />
        </many-to-one>
			
			
			
	</class>
</hibernate-mapping>  

